package com.udemy.spring.spring4;

import org.springframework.context.support.ClassPathXmlApplicationContext;

public class FortuneFromTextApp {

	public static void main(String[] args) {
		
		
		ClassPathXmlApplicationContext  context = new ClassPathXmlApplicationContext("applicatiionContext.xml");
		
		Coach coach = context.getBean("soccerCoach", Coach.class);
		
		coach.getDailyFortune();
		

	}

}
